set keymap vi
j:backward-char
k:next-history
h:previous-history
"g~":upcase-word
"-":vi-arg-digit
",u":"bg~"
"\t":complete
set keymap vi-insert
"\C-a":upcase-word
"\C-z":vi-bword
"\C-b":unix-word-rubout
"\C-w":backward-kill-word
"\C-l":"\C-z\C-a"
"\C-x\C-r":re-read-init-file
"\C-xa":alias-expand-line
"\C-xh":history-expand-line
"\C-xs":shell-expand-line
"\C-xt":tilde-expand
"\C-xg":glob-expand-word
"\C-xp":glob-list-expansions


#set visible-stats off
#set mark-directories on
#set show-all-if-ambiguous off
#set meta-flag on
set input-meta on
set output-meta on
set convert-meta off
set completion-ignore-case on
set editing-mode vi
